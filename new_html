     $scope.getTradeManager=function(){
    		$scope.users.name=$scope.Name;
            //alert("called");
         //   $scope.registerUser();
            data= {
				  "jsonrpc": "2.0",
				  "method": "query",
				  "params": {
				    "type": 1,
				    "chaincodeID": {
